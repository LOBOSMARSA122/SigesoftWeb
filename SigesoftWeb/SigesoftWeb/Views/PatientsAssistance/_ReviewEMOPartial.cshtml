
<script src="~/Scripts/jquery.min.js"></script>
<script src="~/Scripts/pdfobject.min.js"></script>

@{ 
    string DateOrGroup = "";
    string DiseasesName = "";
    string AntecedentTypeName = "";

    if (ViewBag.Antecedent != null)
    {
        DateOrGroup = ViewBag.Antecedent.v_DateOrGroup;
        DiseasesName = ViewBag.Antecedent.v_DiseasesName;
        AntecedentTypeName = ViewBag.Antecedent.v_AntecedentTypeName;
    }
}
 
<div class="col-12">
    <div class="row w-100 p-0 m-0">
        <div class="col-12 mb-2 ml-0 mr-0 p-0 p-lg-1 col-lg-4">
            <div class="row titleRow p-0 m-0">
                <div class="col-12 p-0 m-0" onclick="ColapseTable('DivNotices','heightDivNotices')">
                    <div class="ml-2">
                        Antecedentes
                    </div>
                </div>
            </div>
            <div class="row p-0 m-0">
                <div id="DivNotices" class="col-12 View p-0 m-0">
                    <table class="table tableNotice table-bordered table-sm">
                        <thead>
                            <tr class="text-center">
                                <th scope="col">Descripción</th>
                                <th scope="col">Tipo de atención</th>
                                <th scope="col">Fecha/Grupo</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ 
                                var item = ViewBag.Antecedent;
                                for (int i = 0; i < item.Count; i++)
                                {
                                    <tr>
                                        <td>@item.DiseasesName</td>
                                        <td>@item.AntecedentTypeName</td>
                                        <td>@item.DateOrGroup</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
